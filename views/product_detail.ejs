<%- include admin_header %>
<div class="mainBlock">
    <div class="row">
        <div class="col-md-3"><button type="button" class="btn btn-default">保存商品</button></div>
        <div class="col-md-3"><button type="button" class="btn btn-default">添加属性</button></div>
        <div class="col-md-3"><button type="button" class="btn btn-default">删除属性</button></div>
        <div class="col-md-3"><button type="button" class="btn btn-default">取消</button></div>
    </div>

    <div>
        <form class="form-horizontal" role="form">
            <table class="table">
                <tr>
                    <% var sum =0;for (value in this_product){
                    if(sum != 0){%>
                    <td><%= value%></td><td><input type="text" name="<%= value%>" size="20" value="<%= this_product[value] %>"> </td>
                    <% if(sum %2 ==0){%></tr><tr><% }%>
                    <% }sum +=1}%>
                </tr>
            </table>

        </form>
    </div>

</div>
<%- include admin_footer%>